{
  "name": "drilld",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "amplify:dev": "npx ampx sandbox",
    "amplify:deploy": "npx ampx deploy --branch main",
    "amplify:deploy:prod": "npx ampx deploy --branch production",
    "amplify:delete": "npx ampx delete",
    "amplify:generate": "npx ampx generate outputs --app-id $AMPLIFY_APP_ID --branch $AMPLIFY_BRANCH",
    "postinstall": "npm run amplify:generate"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "@tailwindcss/vite": "^4.1.11",
    "aws-amplify": "^6.15.3",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.16.1",
    "@aws-amplify/backend-auth": "^1.7.1",
    "@aws-amplify/backend-cli": "^1.8.0",
    "@aws-amplify/backend-data": "^1.6.1",
    "@aws-amplify/backend-function": "^1.14.1",
    "@aws-amplify/backend-storage": "^1.4.1",
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
